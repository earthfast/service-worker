{
  "name": "earthfast-sw",
  "dependencies": {
    "rxjs": "^7.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/angular": "^1.8.4",
    "@types/jasmine": "^4.0.3",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.59",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "clang-format": "^1.8.0",
    "cypress": "^10.2.0",
    "cypress-msw-interceptor": "^2.0.3",
    "eslint": "^8.17.0",
    "eslint-plugin-cypress": "^2.12.1",
    "isomorphic-fetch": "^3.0.0",
    "jasmine": "^4.1.0",
    "node-fetch": "^2.6.7",
    "rollup": "^2.63.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.6.2",
    "wait-on": "^6.0.1"
  },
  "scripts": {
    "build": "npm run lint && rollup -c --failAfterWarnings",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" ts-node-dev --require=./register.js --project=tsconfig.test.json ./node_modules/jasmine/bin/jasmine --config=./jasmine.json",
    "test:watch": "ts-node-dev --respawn ./node_modules/jasmine/bin/jasmine --config=./jasmine.json",
    "test:e2e": "npm run build && npm run cypress:run",
    "lint": "eslint src/service-worker --ext .ts",
    "format": "clang-format -i src/service-worker/**/*.ts",
    "format:dryrun": "clang-format -n src/service-worker/**/*.ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headed",
    "cypress:run:debug": "cypress run --browser chrome --headed --no-exit"
  }
}
